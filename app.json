{
  "expo": {
    "name": "Slick",
    "slug": "slick",
    "scheme": "slick",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/newIcon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6699ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.slick.slick",
      "versionCode": 3,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "slick",
              "host": "share"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.slick.slick",
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "whatsapp"
        ]
      }
    },
    "description": ""
  }
}
